<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Results Search</title>
    <link rel="shortcut icon" href="https://www.flaticon.com/premium-icon/icons/svg/2706/2706805.svg" />
    <link rel="stylesheet" href="/public/styles/theme.css">
</head>
<body>
    <header>
        
    </header>
    <main>
        <% teamSearch.forEach(result => { %>
            <section id="section">
                <img id="badge" src="<%= result.badge%>">
                <img id="logo" src="<%= result.logo %>">
                <p id="team"><%= result.team %></p>
                <p id="formed"><%= result.formed%></p>
                <p id="sport"><%= result.sport%></p>
                <p id="league"><%= result.league %></p>
                <p id="stadium"><%= result.stadium%></p>
                <img id="staImg" src="<%= result.stadiumImg%>">
                <p id="staLocation"><%= result.stadiumLocation%></p>
                <a id="website" href="http://<%=result.website%>" target="_blank"> <%= result.team %> Website </a>
                <p id="description"><%= result.teamDescription%></p>
                <img id="clothes"src="<%= result.clothes%>">
            </section>
            <% }) %>
    </main>
    <footer>

    </footer>
</body>
</html>